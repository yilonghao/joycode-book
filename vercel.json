{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "_build/html"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/_build/html/$1"
    }
  ],
  "build": {
    "env": {
      "BUILDCOMMAND": "pip install -r requirements.txt && jupyter-book build ."
    }
  }
}
